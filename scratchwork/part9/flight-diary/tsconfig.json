{
  "compilerOptions": {
    "outDir": "./build/",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  }
}
